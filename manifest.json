{
  "name": "Stymieverse Extension",
  "version": "1.0.0",
  "description": "A sample extension",
  "manifest_version": 3,
    "background": {
        "service_worker": "src/background.js"
    },  
    "content_scripts": [
        {
        "matches": ["<all_urls>"],
        "js": ["src/content.js"]
        }

    ],
  "icons": {
    "16": "icon1.png"
    
  },
    
  

  "permissions": ["storage", "activeTab"],

  "action": {
    "default_popup": "index.html"
  }
}
